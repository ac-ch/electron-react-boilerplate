{
  "name": "boilerplate-electron-react",
  "version": "1.0.0",
  "description": "A boilerplate for Electron using React",
  "main": "main.js",
  "scripts": {
    "build": "webpack -p --config webpack.prod.config.js",
    "dev": "webpack --watch --config webpack.dev.config.js",
    "lint": "./scripts/lint",
    "lint:fix": "./scripts/lint-fix",
    "prepack": "npm run build",
    "pack": "node build.js",
    "start": "NODE_ENV=development electron .",
    "test": "./scripts/test",
    "test:verbose": "./scripts/test-verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djizco/boilerplate-electron-react.git"
  },
  "author": {
    "name": "Rico Rojas",
    "email": "djizco@yahoo.com",
    "url": "https://github.com/djizco"
  },
  "license": "MIT",
  "keywords": [
    "electron",
    "react",
    "redux",
    "webpack",
    "bulma",
    "material-ui",
    "fontawesome",
    "ramda",
    "eslint",
    "code",
    "lab",
    "enzyme"
  ],
  "homepage": "https://github.com/djizco/boilerplate-electron-react#readme",
  "dependencies": {
    "electron": "^1.8.1",
    "electron-packager": "^9.1.0",
    "electron-positioner": "^3.0.0",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bulma": "^0.6.0",
    "classnames": "^2.2.5",
    "code": "^4.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.5-beta.2",
