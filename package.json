    "material-ui": "^0.19.4",
    "moment": "2.18.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.6.0",
    "ramda": "^0.24.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
